{
  "name": "ngx-smz-ui-workspace",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=ngx-smz-ui-demo --port 4103 --configuration=development --open",
    "start:overview": "ng serve --project=ngx-smz-ui-overview --port 4104 --configuration=development --open",
    "start:https:overview": "ng serve --ssl --ssl-key 98348539_localhost.key --ssl-cert 98348539_localhost.cert --project=ngx-smz-ui-overview --port 4104 --configuration=development --open",
    "start:modules": "ng serve --project=ngx-smz-ui-modules --port 4204 --configuration=development --open",
    "start:https:modules": "ng serve --ssl --ssl-key 98348539_localhost.key --ssl-cert 98348539_localhost.cert --project=ngx-smz-ui-modules --port 4107 --configuration=development --open",
    "build": "ng build",
    "lint": "ng lint",
    "build-watch-lib": "ng build ngx-smz-ui --watch",
    "build-prod-lib": "ng build ngx-smz-ui --configuration=production",
    "build-prod-demo": "ng build ngx-smz-ui-demo --configuration=production --stats-json",
    "build-prod-overview": "ng build ngx-smz-ui-overview --configuration=production --stats-json",
    "build-prod-modules": "ng build ngx-smz-ui-modules --configuration=production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/ngx-smz-ui-demo/stats.json --port 8889",
    "rebase": "git pull origin main --rebase",
    "windows:delete-node-modules": "npx rimraf ./**/node_modules",
    "doc": "typedoc --tsconfig tsconfig.docs.json --out docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@auth0/angular-jwt": "^5.2.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@microsoft/signalr": "^6.0.9",
    "@ngneat/tailwind": "^7.0.3",
    "@ngneat/until-destroy": "^9.2.3",
    "@ngxs/router-plugin": "^19.0.0",
    "@ngxs/store": "^19.0.0",
    "@svgdotjs/svg.js": "^3.1.2",
    "@svgdotjs/svg.panzoom.js": "^2.1.2",
    "angular2-uuid": "^1.1.1",
    "animate.css": "^4.1.1",
    "chart.js": "3.8.0",
    "hover.css": "^2.3.2",
    "html2pdf.js": "^0.10.1",
    "jspdf": "^2.4.0",
    "jspdf-autotable": "^3.5.23",
    "lodash-es": "^4.17.21",
    "marked": "^12.0.2",
    "moment": "^2.29.4",
    "nanoid": "3.3.8",
    "ngx-device-detector": "^9.0.0",
    "ngx-highlightjs": "^7.0.1",
    "ngx-pipes": "^3.2.2",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^18.0.2",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "svg-pan-zoom": "^3.6.1",
    "tslib": "^2.3.0",
    "tus-js-client": "^3.1.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.15",
    "@angular/cli": "^18.2.15",
    "@angular/compiler-cli": "^18.2.13",
    "@ngxs/devtools-plugin": "^19.0.0",
    "@types/chart.js": "^2.9.37",
    "@types/lodash-es": "^4.17.6",
    "@types/marked": "^6.0.0",
    "@types/node": "^18.7.23",
    "ng-packagr": "^18.2.1",
    "tailwindcss": "^3.4.1",
    "typedoc": "^0.26.7",
    "typescript": "~5.5.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}
