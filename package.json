  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=ngx-smz-ui-demo --port 4103 --configuration=development",
    "start:overview": "ng serve --project=ngx-smz-ui-overview --port 4104 --configuration=development",
    "start:https:overview": "ng serve --ssl --ssl-key 98348539_localhost.key --ssl-cert 98348539_localhost.cert --project=ngx-smz-ui-overview --port 4104 --configuration=development",
    "start:notifications": "ng serve --project=ngx-smz-ui-notifications --port 4105 --configuration=development",
    "start:dark-theme": "ng serve --project=ngx-smz-ui-dark-theme --port 4106 --configuration=development",
    "start:modules": "ng serve --project=ngx-smz-ui-modules --port 4107 --configuration=development",
    "build": "ng build",
    "lint": "ng lint",
    "build-watch-lib": "ng build ngx-smz-ui --watch",
    "build-prod-lib": "ng build ngx-smz-ui --configuration=production",
    "build-prod-demo": "ng build ngx-smz-ui-demo --configuration=production --stats-json",
    "build-prod-overview": "ng build ngx-smz-ui-overview --configuration=production --stats-json",
    "build-prod-modules": "ng build ngx-smz-ui-modules --configuration=production --stats-json",
