{
  "name": "ngx-smz-ui-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:docs": "ng serve --project docs --port 4200",
    "start:playground": "ng serve --project playground --port 4201 --no-hmr",
    "start:overview": "ng serve --project overview --port 4104 --no-hmr",
    "start:demos": "ng serve --project demos --port 4105 --no-hmr",
    "build:overview": "ng build --project overview",
    "build:demos": "ng build --project demos",
    "build": "ng build",
    "build-watch:smz-ui": "ng build ngx-smz-ui --watch --configuration development",
    "build-watch:smz-ui-layout": "ng build ngx-smz-ui-layout --watch --configuration development",
    "build-prod-smz-ui": "ng build ngx-smz-ui --configuration=production",
    "build-prod-smz-ui-layout": "ng build ngx-smz-ui-layout --configuration=production",
    "windows:delete-node-modules": "npx rimraf ./**/node_modules"
  },
  "private": true,
