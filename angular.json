{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-smz-ui": {
      "projectType": "library",
      "root": "projects/ngx-smz-ui",
      "sourceRoot": "projects/ngx-smz-ui/src",
      "prefix": "smz-ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-smz-ui/tsconfig.lib.json",
            "project": "projects/ngx-smz-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-smz-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-smz-ui/tsconfig.lib.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-smz-ui-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-smz-ui-demo",
      "sourceRoot": "projects/ngx-smz-ui-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-smz-ui-demo",
            "index": "projects/ngx-smz-ui-demo/src/index.html",
            "main": "projects/ngx-smz-ui-demo/src/main.ts",
            "polyfills": "projects/ngx-smz-ui-demo/src/polyfills.ts",
            "tsConfig": "projects/ngx-smz-ui-demo/tsconfig.json",
            "allowedCommonJsDependencies": [
              "jspdf",
              "canvg",
              "raf",
              "core-js"
            ],
            "aot": true,
            "assets": [
              "projects/ngx-smz-ui-demo/src/favicon.ico",
              "projects/ngx-smz-ui-demo/src/assets",
              {
                "glob": "**/*",
                "input": "dist/ngx-smz-ui/resources",
                "output": "/"
              }
            ],
            "styles": [
              "projects/ngx-smz-ui-demo/src/styles.scss",
              "dist/ngx-smz-ui/resources/assets/scss/smz-ui.scss",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeflex/primeflex.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/highlight.js/styles/vs2015.css"
            ],
            "scripts": []
          },
          "configurations": {
            "development": {
              "optimization": {
                "scripts": false,
                "styles": {
                  "minify": false,
                  "inlineCritical": false
                },
                "fonts": false
              },
              "buildOptimizer": false
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ngx-smz-ui-demo/src/environments/environment.ts",
                  "with": "projects/ngx-smz-ui-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "100mb",
                  "maximumError": "200mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "600kb",
                  "maximumError": "1000kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-smz-ui-demo:build"
          },
          "configurations": {
            "development": {
              "browserTarget": "ngx-smz-ui-demo:build:development"
            },
            "production": {
              "browserTarget": "ngx-smz-ui-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-smz-ui-demo:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-smz-ui-demo/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
