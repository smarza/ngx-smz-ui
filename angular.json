          "configurations": {
            "production": {
              "buildTarget": "ngx-smz-ui-overview:build:production"
            },
            "development": {
              "buildTarget": "ngx-smz-ui-overview:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
