{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cronos": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/cronos",
      "sourceRoot": "projects/cronos/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": ["color-functions", "global-builtin", "import", "mixed-decls"]
              }
            },
            "outputPath": "dist/cronos",
            "index": "projects/cronos/src/index.html",
            "browser": "projects/cronos/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/cronos/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/cronos/public"
              },
              {
                "glob": "**/*",
                "input": "dist/ngx-smz-ui/resources",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/primeng/resources/themes",
                "output": "/assets/themes/"
              }
            ],
            "styles": [
              "projects/cronos/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "baseHref": "/cronos/",
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "cronos:build:production"
            },
            "development": {
              "buildTarget": "cronos:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/cronos/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/cronos/public"
              }
            ],
            "styles": [
              "projects/cronos/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ngx-smz-ui": {
      "projectType": "library",
      "root": "projects/ngx-smz-ui",
      "sourceRoot": "projects/ngx-smz-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-smz-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-smz-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-smz-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-smz-ui/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngx-smz-ui-layout": {
      "projectType": "library",
      "root": "projects/ngx-smz-ui-layout",
      "sourceRoot": "projects/ngx-smz-ui-layout/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-smz-ui-layout/ng-package.json"
